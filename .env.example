# Fresas Standalone - Environment Variables
# Copy this to .env and edit as needed

# Path to Excel file (source of truth)
EXCEL_PATH=/app/data/Control FRESAS.xls

# Pending consumos log (fallback when Excel locked)
PENDING_LOG_PATH=/app/data/pending_consumos.csv

# Auth
ADMIN_PIN=1234
JWT_SECRET=change-this-secret-in-production

# Logging
LOG_LEVEL=INFO
